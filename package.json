{
  "name": "rollup-starter-app",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.0.1",
    "@rollup/plugin-node-resolve": "^7.0.0",
    "@rollup/plugin-replace": "^2.3.1",
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.16.2",
    "rollup-plugin-postcss": "^2.5.0",
    "rollup-plugin-terser": "^5.0.0",
    "serve": "^11.0.2",
    "tailwindcss": "^1.2.0"
  },
  "dependencies": {
    "date-fns": "^1.30.1",
    "events": "^3.1.0",
    "expr-eval": "^2.0.2",
    "hooked-elements": "^1.0.1",
    "lit-html": "^1.2.1",
    "lodash": "^4.17.15",
